@model EmployeeWebApplication.Models.Employee

@{
    ViewBag.Title = "Details";
}

<h2>Employee Details</h2>

<table>
    <tr>
        <td>Employee ID: </td>
        <td>@Model.EmployeeId</td>
    </tr>

    <tr>
        <td>Name: </td>
        <td>@Model.Name</td>
    </tr>

    <tr>
        <td>Gender: </td>
        <td>@Model.Gender</td>
    </tr>

    <tr>
        <td>City: </td>
        <td>@Model.City</td>
    </tr>

</table>
